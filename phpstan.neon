includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 8 is the highest level
    level: 5

    #ignoreErrors:

    excludePaths:
        - ./*/*/FileToBeExcluded.php
        - app/Nova/*
        - ./vendor
        - app/Http/Middleware/Authenticate.php
        - app/Services/FirestoreService.php
        - vendor/symfony/*

    checkMissingIterableValueType: false